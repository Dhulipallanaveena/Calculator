while True:
    choice=int(input())
    if choice in(1,2,3,4,5):
        a=int(input())
        b=int(input())
        if choice==1:
            print(a+b)
        elif choice==2:
            print(a-b)
        elif choice==3:
            print(a*b)
        elif choice==4:
            print(a//b)
        elif choice==5:
            print(a%b)
    elif choice==6:
        break
    else:
        print("Invalid Operation")
        
            
        
